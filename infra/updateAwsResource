#!/bin/bash 

AwsHost=${AWSHost}

aws --region cn-north-1 cloudformation update-stack --stack-name heartbeat --template-body file://infra/cloudformation.yml --parameters ParameterKey=GitHubOrg,ParameterValue=au-heartbeat ParameterKey=RepositoryName,ParameterValue=HeartBeat ParameterKey=AwsHost,ParameterValue=${AwsHost} --capabilities CAPABILITY_NAMED_IAM